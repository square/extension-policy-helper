{
  "manifest_version": 2,
  "name": "Extension Policy Helper",
  "version": "1.0.11",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Finds Chrome Extensions that would be blocked by corporate Chrome extension protections.",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "options_page": "options.html",
  "permissions": [
    "management",
    "notifications",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "storage": {
    "managed_schema": "managed_schema.json"
  }
}
